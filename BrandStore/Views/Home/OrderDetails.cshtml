@model IEnumerable<BrandStore.Models.BasketItem>


<section class="static about-sec">
    <div class="container">
        <h1>Siparis Detaylari</h1>
                @foreach (var item in Model)
                {
                <div class="card">
                    <h5 class="card-header">@item.Product.Name</h5>
                    <div class="card-body">
                        <h5 class="card-title">@item.Product.Price</h5>
                        <p class="card-text">@item.Product.Description</p>
                        <img class="img-fluid" href="@item.Product.MainPhoto" />
                    </div>
                </div>
                }
            
    </div>
</section>

