                    <div class="col-md-3">
                        <div class="header-logo">
                            <a href="#" class="logo">
                                <!--<img src="./img/logo.png" alt=""> -->
                                <h1 style="color:white;">BrandStore</h1>
                            </a>
                        </div>
                    </div>
                    <!-- /LOGO -->
                    <!-- SEARCH BAR -->
                    <div class="col-md-6">
                        <div class="header-search">
                            <form>
                                <select class="input-select">
                                    <option value="0">Kategoriler</option>
                                    <option value="1">Kiyafet</option>
